{
  "private": true,
  "scripts": {
    "postinstall": "lerna bootstrap",
    "start": "webpack-dev-server --config docs/webpack.config.js",
    "prebuild": "lerna exec -- rimraf dist",
    "build": "rollup -c",
    "clean": "lerna exec -- rimraf dist && lerna clean",
    "lint": "eslint --fix --ignore-path .gitignore .",
    "publish": "lerna exec -- npm publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint --",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^3.5.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.1.0",
    "husky": "^1.3.1",
    "lerna": "^3.10.5",
    "lint-staged": "^8.1.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.0",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-uglify-es": "0.0.1",
    "rollup-plugin-vue": "^4.6.1",
    "style-loader": "^0.23.1",
    "vue": "^2.5.22",
    "vue-loader": "^15.5.1",
    "vue-template-compiler": "^2.5.22",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  }
}
