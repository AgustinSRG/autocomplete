{
  "name": "@trevoreyre/autocomplete",
  "version": "0.0.2",
  "description": "Simple autocomplete component in vanilla JS and Vue",
  "private": false,
  "main": "index.js",
  "files": [
    "*.json",
    "**/*.js",
    "**/*.css"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "author": "Trevor Eyre <trevoreyre@gmail.com> (https://trevoreyre.com)",
  "license": "MIT",
  "repository": "trevoreyre/autocomplete",
  "bugs": "https://github.com/trevoreyre/autocomplete/issues",
  "scripts": {
    "start": "concurrently \"webpack --watch\" \"browser-sync start -c browser-sync.config.js\"",
    "prebuild": "rimraf build",
    "build": "babel src --out-dir build",
    "postbuild": "copyfiles -f package.json src/styles.css build"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "babel-loader": "^8.0.4",
    "browser-sync": "^2.26.3",
    "concurrently": "^4.1.0",
    "copyfiles": "^2.1.0",
    "css-loader": "^2.1.0",
    "html-webpack-plugin": "^3.2.0",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.1.2"
  }
}
