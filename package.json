{
  "name": "@trevoreyre/autocomplete",
  "version": "0.0.3",
  "description": "Simple autocomplete component in vanilla JS and Vue",
  "private": false,
  "main": "index.js",
  "files": [
    "*.json",
    "**/*.js",
    "**/*.css"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "author": "Trevor Eyre <trevoreyre@gmail.com> (https://trevoreyre.com)",
  "license": "MIT",
  "repository": "trevoreyre/autocomplete",
  "bugs": "https://github.com/trevoreyre/autocomplete/issues",
  "scripts": {
    "start": "webpack-dev-server --config docs/webpack.config.js",
    "prebuild": "rimraf build",
    "build": "npm run build-js && npm run build-vue",
    "build-js": "babel src --out-dir build",
    "build-vue": "npm run build-vue-umd && npm run build-vue-es && npm run build-vue-unpkg",
    "build-vue-umd": "cross-env NODE_ENV=production rollup -c -f umd -o build/vue/Autocomplete.umd.js",
    "build-vue-es": "cross-env NODE_ENV=production rollup -c -f es -o build/vue/Autocomplete.es.js",
    "build-vue-unpkg": "cross-env NODE_ENV=production rollup -c -f iife -o build/vue/Autocomplete.min.js",
    "postbuild": "copyfiles -f package.json src/styles.css build"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "babel-loader": "^8.0.4",
    "copyfiles": "^2.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "minimist": "^1.2.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.0",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify-es": "0.0.1",
    "rollup-plugin-vue": "^4.6.1",
    "style-loader": "^0.23.1",
    "vue": "^2.5.22",
    "vue-loader": "^15.5.1",
    "vue-template-compiler": "^2.5.22",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  }
}
